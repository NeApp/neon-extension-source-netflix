{
  "name": "Netflix",
  "version": "0.1.0",

  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/*"
      ],
      "js": [
        "source.netflix/source.netflix.js"
      ]
    }
  ],

  "permissions": {
    "origins": [
      "*://www.netflix.com/*",
      "*://netflix.com/*"
    ]
  },

  "web_accessible_resources": [
    "source.netflix.shim/source.netflix.shim.js"
  ]
}