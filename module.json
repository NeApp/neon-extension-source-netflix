{
  "title": "Netflix",

  "content_scripts": [
    {
      "id": "main",
      "conditions": [
        {
          "pattern": "*://netflix.com/*",
          "pageUrl": {
            "hostEquals": "netflix.com"
          }
        },
        {
          "pattern": "*://www.netflix.com/*",
          "pageUrl": {
            "hostEquals": "www.netflix.com"
          }
        }
      ],
      "js": [
        "common.js",
        "source/common.js",
        "source/netflix/netflix.js"
      ]
    }
  ],
  "services": [
    "configuration",

    "source/activity"
  ],
  "origins": [
    "*://netflix.com/*",
    "*://www.netflix.com/*"
  ],
  "web_accessible_resources": [
    "source/netflix/shim/shim.js"
  ],

  "webpack": {
    "modules": [
      "shim"
    ]
  }
}
