{
  "title": "Netflix",

  "content_scripts": [
    {
      "id": "source.netflix/main",
      "conditions": [
        {
          "pattern": "*://netflix.com/*",
          "pageUrl": {
            "hostEquals": "netflix.com"
          }
        },
        {
          "pattern": "*://www.netflix.com/*",
          "pageUrl": {
            "hostEquals": "www.netflix.com"
          }
        }
      ],
      "js": [
        "vendor.js",
        "shared.js",
        "source/netflix/netflix.js"
      ]
    }
  ],
  "services": [
    "configuration",

    "source/activity",
    "source/sync"
  ],
  "web_accessible_resources": [
    "source/netflix/shim/shim.js"
  ],
  "webpack": {
    "chunks": [
      "shim"
    ]
  }
}
